{
	"jekyll-img": {
		"prefix": "jekyll-img",
		"body": [
			"{% include img.html src=\"$1\" width=$2 desc=\"$3\" border=false %}"			
		],
		"description": "Jekyll image"
	},
	"jekyll-prev": {
		"prefix": "jekyll-prev",
		"body": [
			"{% richpreview $1 %}"			
		],
		"description": "Jekyll rich link preview"
	},
	"jekyll-header": {
		"prefix": "jekyll-header",
		"body": [
			"---",
			"layout: post",
			"title: '$1'",
			"date: '20$2-$3-$4 $5:$6:01 +0100'",
			"---"
			
		],
		"description": "Jekyll post header"
	}
}
