[push]
	default = matching

[core]
	trustctime = false
	filemode = false
	autocrlf = false
	editor = vim
	excludesfile = /Users/sebastiantschoepel/.gitignore
	pager = 

[http]
	sslverify = false

[color]
	ui = true

[credential]
	helper = cache --timeout=3600

[alias]
	cp = cherry-pick
	co = checkout
	cl = clone
	ci = commit
	st = status -sb
	br = branch

[user]
	useConfigOnly = true
